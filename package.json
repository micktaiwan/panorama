{
  "name": "panorama",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings.json",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "dev:meteor": "meteor run --settings settings.json",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "dev:desktop": "concurrently -n meteor,electron -c blue,green \"npm:dev:meteor\" \"npm:dev:electron\""
  },
  "dependencies": {
    "@babel/runtime": "^7.23.5",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@qdrant/js-client-rest": "^1.15.1",
    "@swc/helpers": "^0.5.17",
    "marked": "^12.0.2",
    "meteor-node-stubs": "^1.2.12",
    "node-fetch": "^3.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xlsx": "^0.18.5"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    },
    "modern": true
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "electron": "^38.0.0",
    "wait-on": "^8.0.4"
  },
  "main": "electron/main.js"
}
